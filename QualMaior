 * 
 */
package QualMaior;

import java.util.Scanner;

/**
 * @author icaro
 */
public class Programa4 
{
    int numerointeiro1, numerointeiro2 ;
    
    static Scanner leitura = new Scanner(System.in);
    
    public static void main(String[] args)
    {
        
    System.out.println("Digite o primeiro número inteiro: ");
    int numerointeiro1 = leitura.nextInt();
    
    System.out.println("Digite o segundo número inteiro: ");
    int numerointeiro2 = leitura.nextInt();
    
        if ( numerointeiro1 > numerointeiro2 ) 
        {
            System.out.println("O primeiro número inteiro é maior.");
        }
        else if ( numerointeiro1 < numerointeiro2 ) 
        {
            System.out.println("O segundo número inteiro é maior.");
        }
        else if ( numerointeiro1 == numerointeiro2 ) 
        {
            System.out.println("Os números inteiros são iguais");
        }
    }
}
